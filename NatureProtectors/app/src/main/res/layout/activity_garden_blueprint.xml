        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="40dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/StringValueForImageButton"
        android:rotation="90"
        app:layout_constraintEnd_toEndOf="@+id/garden_blueprint_plan"
        app:layout_constraintStart_toStartOf="@+id/garden_blueprint_plan"
        app:layout_constraintTop_toBottomOf="@+id/garden_blueprint_plan"
        app:srcCompat="@drawable/garden_blueprint_control_grid"
        tools:ignore="TouchTargetSizeCheck" />

