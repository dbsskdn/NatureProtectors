        android:layout_marginBottom="40dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/StringValueForImageButton"
        android:rotation="180"
        app:layout_constraintBottom_toBottomOf="@+id/garden_blueprint_plan"
        app:layout_constraintStart_toEndOf="@+id/garden_blueprint_plan"
        app:layout_constraintTop_toTopOf="@+id/garden_blueprint_plan"
        app:srcCompat="@drawable/garden_blueprint_control_grid"
        tools:ignore="TouchTargetSizeCheck" />

